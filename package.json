{
  "name": "fs-react",
  "version": "1.0.0",
  "description": "full stack react with node, express, react",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV='production' && webpack",
    "start": "concurrently \"npm run serve\" \"npm run dev\"",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "dev": "webpack-dev-server",
    "serve": "nodemon src/server/server --exec babel-node",
    "initialize-db": "babel-node src/server/initialize-db",
    "purge-db": "babel-node src/server/purge-db",
    "reset-db": "npm run purge-db && npm run initialize-db",
    "server-test": "babel-node src/server/test/server.spec",
    "get-data-test": "babel-node src/server/test/getData.spec"
  },
  "keywords": [
    "react",
    "node",
    "express",
    "full",
    "stack"
  ],
  "author": "tahoeRobbo",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "tailwindcss": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "css-loader": "^3.4.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "mongodb": "^3.1.10",
    "postcss": "^7.0.25",
    "postcss-loader": "^3.0.0",
    "standard": "^14.3.1",
    "style-loader": "^1.1.1",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 8888
    }
  }
}
